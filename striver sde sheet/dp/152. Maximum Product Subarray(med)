problem:https://leetcode.com/problems/maximum-product-subarray/
video:
/*
brute force(GETTING TLE)
time:O(n^3)
space:O(1)
*/

#include<bits/stdc++.h>
using namespace std;
class Solution {
public:
    int maxProduct(vector<int>& nums) {
    int ans=INT_MIN;
        for(int i=0;i<nums.size();i++){
            for(int j=i;j<nums.size();j++){
                int k=i;
              int pro=1;
                while(k<=j){
                    pro*=nums[k];
                    k++;
                    
                }
                ans=max(ans,pro);
            }
        }
        return ans;
    }
};
